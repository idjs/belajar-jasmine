{
  "name": "no-skeleton.docpad",
  "version": "0.1.0",
  "description": "New DocPad project without using a skeleton",
  "engines": {
    "node": "0.10",
    "npm": "1.3"
  },
  "dependencies": {
    "docpad": "~6.53.4",
    "docpad-plugin-marked": "~2.1.1",
    "docpad-plugin-eco": "~2.0.2",
    "docpad-plugin-ghpages": "~2.4.0",
    "docpad-plugin-highlightjs": "~2.1.8",
    "docpad-plugin-livereload": "~2.5.3"
  },
  "main": "node_modules/docpad/bin/docpad-server",
  "scripts": {
    "start": "node_modules/docpad/bin/docpad-server",
    "gh-publish": "docpad deploy-ghpages --env static"
  }
}
