---
layout: base
---

<div class="ui red vertical sidebar menu" style="display:block;margin-left: 0!important;">

    <div class="item">
        <h2>Belajar Jasmine</h2>
        <div class="menu">
            <% for doc in @getFilesAtPath("posts").toJSON(): %>
                <a href="/<%= doc.relativeOutPath %>" class="item"><%= doc.title %></a>
            <% end %>
        </div>
    </div>

</div>

<div id="content">
    <%- @content %>
</div>